[
  {
    "context": "Pane",
    "bindings": {
      "cmd-1": ["pane::ActivateItem", 0],
      "cmd-2": ["pane::ActivateItem", 1],
      "cmd-3": ["pane::ActivateItem", 2],
      "cmd-4": ["pane::ActivateItem", 3],
      "cmd-5": ["pane::ActivateItem", 4],
      "cmd-6": ["pane::ActivateItem", 5],
      "cmd-7": ["pane::ActivateItem", 6],
      "cmd-8": ["pane::ActivateItem", 7],
      "cmd-9": ["pane::ActivateItem", 8],
      "cmd-0": "pane::ActivateLastItem",
      "ctrl--": "pane::GoBack",
      "ctrl-_": "pane::GoForward",
      "cmd-shift-t": "pane::ReopenClosedItem",
      "cmd-shift-f": "project_search::ToggleFocus"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-j": "editor::JoinLines",
      "cmd-shift-d": "editor::DuplicateLineDown",
      "ctrl-\\": "pane::SplitRight",
      "cmd-\\": "workspace::ToggleLeftDock",
      "ctrl-m": "editor::MoveToEnclosingBracket",
      "ctrl-n": "editor::MoveToEnclosingBracket",
      "cmd-shift-/": "editor::SelectEnclosingSymbol",
      "cmd-|": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "Editor && mode == full",
    "bindings": {
      "cmd-i": "assistant::InlineAssist"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-w": "pane::CloseActiveItem",
      "cmd-k f": ["task::Spawn", { "task_name": "Open Fork" }],
      "cmd-shift-p": "command_palette::Toggle",
      "cmd-p": "command_palette::Toggle",
      "cmd-t": "file_finder::Toggle",
      "cmd-alt-/": "assistant::ToggleFocus",
      "cmd-?": "editor::SelectEnclosingSymbol",
      "cmd-\\": "workspace::ToggleLeftDock",
      "cmd-b": "workspace::ToggleRightDock",
      "cmd-|": "workspace::ToggleBottomDock",
      "ctrl-1": ["workspace::ActivatePane", 0],
      "ctrl-2": ["workspace::ActivatePane", 1],
      "ctrl-3": ["workspace::ActivatePane", 2],
      "ctrl-4": ["workspace::ActivatePane", 3],
      "ctrl-5": ["workspace::ActivatePane", 4],
      "ctrl-6": ["workspace::ActivatePane", 5],
      "ctrl-7": ["workspace::ActivatePane", 6],
      "ctrl-8": ["workspace::ActivatePane", 7],
      "ctrl-9": ["workspace::ActivatePane", 8]
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "cmd-l": "terminal::Clear"
    }
  }
]
